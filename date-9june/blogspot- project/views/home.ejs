<%- include("partials/header");-%>

<h1>Home</h1>
<p><%= startingContent %></p>
<!-- when you are using <%  %> this tag you don't need to use <%= %> this tag -->

<% allPosts.forEach(function(post){ %>
    <h1><%= post.title %></h1>
    <p><%= post.body.substring(0,100)+"..." %> <a href="/posts/<%=post.title%>"> Read More</a></p>

    <!-- a href also makes a GET request-->
<% }); %>


<%- include("partials/footer");-%>
