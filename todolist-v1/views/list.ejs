<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listtittle %></h1>
    </div>
    
   
    <div class="box">
        <% for (var i=0;i<newlistitem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newlistitem[i] %></p>
            </div>
            
        <% } %>


        <form class="item" action="/" method="post">
            <input class="new" type="text " name="newitem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%=listtittle %>> +</button>
        </form>
    </div>
<%- include("footer") -%>
    
